{
  "expo": {
    "name": "Uha!",
    "slug": "uha",
    "version": "3.0.0",
    "scheme": "uha",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF26BD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.engd.uha"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF26BD"
      },
      "package": "com.engd.uha"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permita que o Uha! acesse sua c√¢mera."
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permita que o Uha! acesse suas fotos.",
          "savePhotosPermission": "Permita que o Uha! salve as fotos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ]
     
  }
}